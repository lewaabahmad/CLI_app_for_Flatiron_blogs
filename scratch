  #presenters

PresentationDate
  #date
  #presentation_group

 presentation_date = PresentationDate.new("Jan 26")
 Presenter.new("Steven Nunez", presentation_date)
 	#initialize
 	  presentation_date.group << self
 	  @date = presentation_date


